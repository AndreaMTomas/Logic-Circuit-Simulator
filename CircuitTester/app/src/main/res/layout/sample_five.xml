<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/s5circuit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sample_5" />

    <CheckBox
        android:id="@+id/s5b1"
        android:layout_width="46dp"
        android:layout_height="44dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="350dp"
        android:layout_marginBottom="212dp"
        android:scaleX="2.5"
        android:scaleY="2.5"
        android:translationX="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit" />

    <CheckBox
        android:id="@+id/s5b2"
        android:layout_width="46dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="349dp"
        android:layout_marginBottom="164dp"
        android:scaleX="2.5"
        android:scaleY="2.5"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit" />

    <CheckBox
        android:id="@+id/s5b3"
        android:layout_width="46dp"
        android:layout_height="44dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="350dp"
        android:layout_marginBottom="122dp"
        android:scaleX="2.5"
        android:scaleY="2.5"
        android:translationX="1dp"
        android:translationY="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit" />

    <CheckBox
        android:id="@+id/s5b4"
        android:layout_width="46dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="216dp"
        android:layout_marginEnd="349dp"
        android:layout_marginBottom="70dp"
        android:scaleX="2.5"
        android:scaleY="2.5"
        android:translationY="-3dp"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit" />

    <ImageView
        android:id="@+id/s5Led"
        android:layout_width="39dp"
        android:layout_height="64dp"
        android:layout_marginStart="372dp"
        android:layout_marginTop="84dp"
        android:layout_marginBottom="182dp"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit"
        app:srcCompat="@drawable/led_off" />

    <Button
        android:id="@+id/sample5Back"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:background="@drawable/button_border"
        android:fontFamily="@font/retro"
        android:gravity="center"
        android:text="Back"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />
</androidx.constraintlayout.widget.ConstraintLayout>