<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/s5circuit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.259"
        app:srcCompat="@drawable/sample_5" />

    <CheckBox
        android:id="@+id/s5b1"
        android:layout_width="46dp"
        android:layout_height="44dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="641dp"
        android:layout_marginBottom="266dp"
        android:scaleX="4"
        android:scaleY="4"
        android:translationX="-1dp"
        android:translationY="-2dp"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit" />

    <CheckBox
        android:id="@+id/s5b2"
        android:layout_width="46dp"
        android:layout_height="44dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="641dp"
        android:layout_marginBottom="182dp"
        android:scaleX="4"
        android:scaleY="4"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit" />

    <CheckBox
        android:id="@+id/s5b3"
        android:layout_width="46dp"
        android:layout_height="44dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="640dp"
        android:layout_marginBottom="100dp"
        android:scaleX="4"
        android:scaleY="4"
        android:translationX="-2dp"
        android:translationY="-1dp"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit" />

    <CheckBox
        android:id="@+id/s5b4"
        android:layout_width="46dp"
        android:layout_height="44dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="262dp"
        android:layout_marginEnd="641dp"
        android:layout_marginBottom="20dp"
        android:scaleX="4"
        android:scaleY="4"
        android:translationY="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit" />

    <ImageView
        android:id="@+id/s5Led"
        android:layout_width="70dp"
        android:layout_height="111dp"
        android:layout_marginStart="661dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="195dp"
        app:layout_constraintBottom_toBottomOf="@+id/s5circuit"
        app:layout_constraintEnd_toEndOf="@+id/s5circuit"
        app:layout_constraintStart_toStartOf="@+id/s5circuit"
        app:layout_constraintTop_toTopOf="@+id/s5circuit"
        app:srcCompat="@drawable/led_off" />

    <Button
        android:id="@+id/sample5Back"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:background="@drawable/button_border"
        android:fontFamily="@font/retro"
        android:gravity="center"
        android:text="Back"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />
</androidx.constraintlayout.widget.ConstraintLayout>